#: ../docs/gateways/mqtt.rst:134
msgid "Gateway 1"
msgstr ""
"게이트웨이 1"

#: ../docs/gateways/mqtt.rst:104
#: ../docs/gateways/mqtt.rst:134
msgid "mygateway"
msgstr ""
"mygateway"

#: ../docs/gateways/mqtt.rst:104
#: ../docs/gateways/mqtt.rst:134
msgid "gateway1"
msgstr ""
"gateway1"

#: ../docs/gateways/mqtt.rst:106
#: ../docs/gateways/mqtt.rst:136
msgid "Device 1"
msgstr ""
"디바이스 1"

#: ../docs/gateways/mqtt.rst:106
#: ../docs/gateways/mqtt.rst:136
msgid "mydevice"
msgstr ""
"mydevice"

#: ../docs/gateways/mqtt.rst:106

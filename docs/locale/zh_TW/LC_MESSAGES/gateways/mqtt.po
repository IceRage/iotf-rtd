#: ../docs/gateways/mqtt.rst:139
msgid "Gateway 1"
msgstr ""
"閘道 1"

#: ../docs/gateways/mqtt.rst:109
#: ../docs/gateways/mqtt.rst:139
msgid "mygateway"
msgstr ""
"mygateway"

#: ../docs/gateways/mqtt.rst:109
#: ../docs/gateways/mqtt.rst:139
msgid "gateway1"
msgstr ""
"gateway1"

#: ../docs/gateways/mqtt.rst:111
#: ../docs/gateways/mqtt.rst:141
msgid "Device 1"
msgstr ""
"裝置 1"

#: ../docs/gateways/mqtt.rst:111
#: ../docs/gateways/mqtt.rst:141
msgid "mydevice"
msgstr ""
"mydevice"

#: ../docs/gateways/mqtt.rst:111

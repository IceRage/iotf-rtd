msgid "Gateway 1"
msgstr ""
"Passerelle 1"

#: ../docs/gateways/mqtt.rst:109
#: ../docs/gateways/mqtt.rst:139
msgid "mygateway"
msgstr ""
"mapasserelle "

#: ../docs/gateways/mqtt.rst:109
#: ../docs/gateways/mqtt.rst:139
msgid "gateway1"
msgstr ""
"passerelle1"

#: ../docs/gateways/mqtt.rst:111
#: ../docs/gateways/mqtt.rst:141
msgid "Device 1"
msgstr ""
"Terminal 1 "

